s are ALWAYS available */\n                    }\n                    \n                    <Route>\n            { /*If none of the other pages match, redirect them to the main page */}\n                        <Redirect to=\"/LandingPage\" />\n                    </Route>\n                </Switch>\n\n        </Router>\n      \n    );\n}\n\nexport default App;\n",["42","43"],"C:\\Users\\epayn\\Sept2020\\Project3\\PIFA\\client\\src\\utils\\GlobalStore.js",[],{"ruleId":"44","severity":1,"message":"45","line":5,"column":8,"nodeType":"46","messageId":"47","endLine":5,"endColumn":14},{"ruleId":"48","severity":1,"message":"49","line":3,"column":1,"nodeType":"50","endLine":32,"endColumn":2},{"ruleId":"44","severity":1,"message":"51","line":8,"column":12,"nodeType":"46","messageId":"47","endLine":8,"endColumn":17},{"ruleId":"44","severity":1,"message":"52","line":2,"column":50,"nodeType":"46","messageId":"47","endLine":2,"endColumn":54},{"ruleId":"53","severity":1,"message":"54","line":39,"column":8,"nodeType":"55","endLine":39,"endColumn":10,"suggestions":"56"},{"ruleId":"44","severity":1,"message":"57","line":41,"column":11,"nodeType":"46","messageId":"47","endLine":41,"endColumn":17},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},"no-unused-vars","'Button' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","'state' is assigned a value but never used.","'Link' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["62"],"'logout' is assigned a value but never used.","no-native-reassign",["63"],"no-negated-in-lhs",["64"],{"desc":"65","fix":"66"},"no-global-assign","no-unsafe-negation","Update the dependencies array to be: [dispatch]",{"range":"67","text":"68"},[1478,1480],"[dispatch]"]
